# php syntax highlighting file
set keywords(function) { break case class continue declare default define do echo else elseif endif endwhile extends false \
    for foreach function global if include include_once new null private protected public require require_once return static \
    switch true while exit constant implements create \
}

set keywords(command) { $this parent __construct use namespace self in_array array const try catch as unset }
set keywords(tag) { <?php <div </div <p <p> </p> <body </body> <html </html> }
set keywords(constant) { __FILE__ __METHOD__ __DIR__ _SERVER }
set keywords(control) {}
set keywords(string) {}
set keywords(text) {}
set keywords(comment) {}
set keywords(brace) {}
set keywords(variable) {}
